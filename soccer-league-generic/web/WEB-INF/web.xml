<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>soccer-league-generic</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description>developing view component using GenericServlet</description>
    <display-name>LisLeague</display-name>
    <servlet-name>LisLeague</servlet-name>
    <servlet-class>view.LisLeagueServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LisLeague</servlet-name>
    <url-pattern>/list_leagues.view</url-pattern>
  </servlet-mapping>
</web-app>