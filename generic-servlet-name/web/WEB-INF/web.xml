<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>generic-servlet-name</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description>Generic Servlet Example</description>
    <display-name>NameServletView</display-name>
    <servlet-name>NameServletView</servlet-name>
    <servlet-class>view.NameServletView</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>NameServletView</servlet-name>
    <url-pattern>/process</url-pattern>
  </servlet-mapping>
</web-app>